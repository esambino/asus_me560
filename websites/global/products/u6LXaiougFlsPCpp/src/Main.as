package  {		import flash.display.MovieClip;	import flash.external.ExternalInterface;	import flash.events.Event;	public class Main extends MovieClip {						public function Main() {			// constructor code			this.addEventListener(Event.ADDED_TO_STAGE,init);		}				private function init(e:Event):void		{			ExternalInterface.addCallback('display',display);			ExternalInterface.addCallback('destroy',destroy);		}				public function destroy():void		{			this.gotoAndStop('destroy');		}				public function display():void		{			this.gotoAndPlay('display');		}	}	}